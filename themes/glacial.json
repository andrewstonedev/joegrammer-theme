            "foreground": "#275DAD",
            "italic": true
        },
        "variable": "#1A3A3A",
        "variable.defaultLibrary": "#525692",
        "variable.reference": {
            "italic": true
        },
        "variable.declaration.static.constant.public": {
            "fontStyle": ""
        },
        "type": "#1A3A3A",
        "builtinType": "#1A3A3A",
        "number": "#8A3258",
        "boolean": "#8A3258",
        "formatSpecifier": {
            "italic": true
        },
        "builtinConstant:python": "#8A3258",
        "selfKeyword": {
            "foreground": "#1A3A3A",
            "italic": true
        },
        "interface": "#5863F8",
        "decorator": "#5863F8",
        "tomlArrayKey": "#364958D0",
        "tomlTableKey": "#364958D0",
        "parameter": {
            "italic": true
        }
