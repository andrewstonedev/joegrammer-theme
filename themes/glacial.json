            "foreground": "#7D8F7A",
            "italic": true
        },
        "string": "#32936F",
        "function": "#275DAD",
        "function.defaultLibrary": {
            "italic": true
        },
        "struct": "#8A3258",
        "enum": "#0D6B70",
